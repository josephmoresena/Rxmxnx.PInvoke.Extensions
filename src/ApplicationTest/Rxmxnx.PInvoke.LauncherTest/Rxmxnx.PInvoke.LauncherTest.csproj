<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>$(DefaultFramework)</TargetFramework>
        <OutputType>Exe</OutputType>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <PublishAot>true</PublishAot>
        <InvariantGlobalization>true</InvariantGlobalization>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
        <PublishDir Condition="'$(PublishDir)' == ''">..\..\..\</PublishDir>
        <CopyOutputSymbolsToPublishDirectory>false</CopyOutputSymbolsToPublishDirectory>
        <CheckEolTargetFramework>false</CheckEolTargetFramework>
        <NoWarn>$(NoWarn);NU1801;NU1102</NoWarn>
    </PropertyGroup>

    <ItemGroup>
        <AssemblyAttribute Include="System.Diagnostics.CodeAnalysis.ExcludeFromCodeCoverage"/>
    </ItemGroup>

    <ItemGroup>
        <Using Include="System.Buffers"/>
        <Using Include="System.Collections.Concurrent"/>
        <Using Include="System.Collections.ObjectModel"/>
        <Using Include="System.Collections.Specialized"/>
        <Using Include="System.Diagnostics"/>
        <Using Include="System.Diagnostics.CodeAnalysis"/>
        <Using Include="System.IO.Compression"/>
        <Using Include="System.Runtime.InteropServices"/>
        <Using Include="System.Text"/>

        <Using Include="Rxmxnx.PInvoke.ApplicationTest.Util"/>
    </ItemGroup>

</Project>
