<Project Sdk="Microsoft.NET.Sdk.Web">

    <Import Project="..\PackageReference.props"/>
    <Import Project="..\DotnetCoreApp.props"/>

    <PropertyGroup>
        <TargetFramework>$(DefaultFramework)</TargetFramework>
        <InvariantGlobalization>true</InvariantGlobalization>
        <ImplicitUsings>true</ImplicitUsings>
        <Nullable>enable</Nullable>
        <PublishTrimmed>true</PublishTrimmed>
        <TrimMode>full</TrimMode>
        <TrimmerSingleWarn>false</TrimmerSingleWarn>
        <EnableTrimAnalyzer>true</EnableTrimAnalyzer>
        <InterceptorsNamespaces>$(InterceptorsNamespaces);Microsoft.AspNetCore.OpenApi.Generated</InterceptorsNamespaces>
        <NoWarn>$(NoWarn);IL2026;IL2070;IL2072;IL2075;IL3050</NoWarn>
    </PropertyGroup>

    <Import Condition="$([MSBuild]::IsTargetFrameworkCompatible('$(TargetFramework)', 'net10.0'))" Project="..\OpenApi2.props"/>
    <Import Condition="!$([MSBuild]::IsTargetFrameworkCompatible('$(TargetFramework)', 'net10.0'))" Project="..\OpenApi1.props"/>

    <Import Project="..\Artifact.targets"/>

</Project>
